#trajectory-container >>> .cesium-popup-content-wrapper {
	text-align: center;
	max-height: 200px;
	overflow-y: auto;
	font-size: 14px;
	text-align: left;
	border-radius: 4px;
	padding-top: 5px;
	padding-left: 20px;
	padding-bottom: 20px;
	padding-right: 20px;
}

#trajectory-container >>> .cesium-prompt-content-wrapper {
	text-align: center;
	max-height: 200px;
	overflow-y: auto;
	box-shadow: 0 3px 14px rgba(0, 0, 0, 0.4);
	padding: 1px;
	text-align: left;
}

#trajectory-container >>> .cesium-popup-content {
	line-height: 1.4;
	/* background: rgba(250, 170, 35, 0.6);
	border: 1px solid rgba(250, 170, 35, 1); */
	cursor: pointer;
}
#trajectory-container >>> .cesium-popup-content.selected {
	background: rgba(255, 0, 0, 0.863) !important;
	border: 1px solid rgba(255, 0, 0, 1) !important;
}

#trajectory-container >>> .popupContent > div {
	padding-right: 25px;
	padding-left: 5px;
}

#trajectory-container >>> .popupContent{
	color: white;
}
#trajectory-container >>> .promptDiv{
	background: rgba(0, 0, 0, 0.4);
	font-size: 10px;
}

#trajectory-container >>> .promptContent{
	color:white;
}