/*leaflet风格气泡窗口样式模板*/
#trajectory-container >>> .cesium-popup {
	position: absolute;
	text-align: center;
	background: url('../../../static/images/line.png')  left bottom / 20px 20px  no-repeat;
}
#trajectory-container >>> .cesium-popup::before {
	content: "";
	height: 1px;
	position: absolute;
	left: 20px;
	right: 20px;
	bottom: 17px;
	background: #f4aa23;
}

