.el-picker-panel,
.el-input__inner,
.el-input.is-disabled .el-input__inner,
.el-year-table td .cell,
.el-month-table td .cell {
  font-size: 14px;
  color: rgba(138, 226, 255, 1);
  background: rgba(0, 77, 102, 1);
  border: 1px solid rgba(0, 143, 189, 1);
}

.el-date-table td.in-range div,
.el-date-table td.in-range div:hover,
.el-date-table.is-week-mode .el-date-table__row.current div,
.el-date-table.is-week-mode .el-date-table__row:hover div {
  background-color: rgb(12, 110, 120);
}

.el-button+.el-button,
.el-time-panel__btn.confirm {
  background-color: rgb(12, 180, 110);
  color: #fff;
  border: 1px solid rgb(12, 180, 110);
  line-height: 24px;
  padding: 0 10px;
  margin: 0 5px;
  border-radius: 4px;
}

.el-button.is-plain:focus,
.el-button.is-plain:hover {
  background-color: rgba(0, 143, 189, 1);
  border-color: rgba(0, 143, 189, 1);
  color: #fff;
}

.el-button.is-disabled.is-plain,
.el-button.is-disabled.is-plain:focus,
.el-button.is-disabled.is-plain:hover {
  background-color: #ddd;
  border-color: #ddd;
  color: #999;
}

.el-picker-panel__footer {
  border-top: 1px solid rgba(0, 143, 189, 1);
  background: rgba(0, 77, 102, 1);
}

.el-date-range-picker__content.is-left {
  border-right: 1px solid rgba(0, 143, 189, 1);
}

.el-date-range-picker__time-header {
  border-bottom: 1px solid rgba(0, 143, 189, 1);
}

.el-date-editor .el-range-separator,
.el-date-range-picker__time-header>.el-icon-arrow-right {
  color: #fff;
}

.el-date-editor .el-range-input {
  color: rgba(138, 226, 255, 1);
  background: transparent;
}

.el-date-picker__header-label,
.el-picker-panel__icon-btn,
.el-date-table th {
  color: #fff;
}

.el-popper .popper__arrow,
.el-popper .popper__arrow::after {
  border-style: none;
}

.el-popper[x-placement^="bottom"] {
  margin-top: 5px;
}

.el-date-picker__time-header {
  border-bottom: none;
}

.el-date-table td.disabled div,
.el-year-table td.disabled .cell,
.el-month-table td.disabled .cell {
  background-color: rgba(0, 77, 102, 1);
  color: rgba(255, 255, 255, 0.15);
}

.el-time-panel {
  right: 0;
}

.el-scrollbar {
  background-color: rgba(0, 77, 102, 1);
}

.el-time-spinner__item {
  color: #fff;
}

.el-time-spinner__item.active:not(.disabled) {
  color: rgba(138, 226, 255, 1);
}

.el-time-panel {
  border: 1px solid rgba(0, 143, 189, 1);
  left: -30px;
}

.el-time-panel__footer {
  border-top: 1px solid rgba(0, 143, 189, 1);
  background-color: rgba(0, 77, 102, 1);
}

.el-time-panel__btn.cancel {
  color: #fff;
}

.el-time-spinner__item:hover:not(.disabled):not(.active) {
  background: rgba(0, 77, 102, 1);
  color: #409EFF;
  cursor: pointer;
}

.el-date-table th {
  border-bottom: 1px solid rgba(0, 143, 189, 1);
}
